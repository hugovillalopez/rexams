{
  "name": "@milkdown/theme-nord",
  "type": "module",
  "version": "7.5.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Milkdown/milkdown.git",
    "directory": "packages/plugins/utils"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.es.js"
    },
    "./style.css": "./lib/style.css"
  },
  "main": "./lib/index.es.js",
  "files": [
    "lib",
    "src"
  ],
  "peerDependencies": {
    "@milkdown/core": "^7.2.0",
    "@milkdown/ctx": "^7.2.0",
    "@milkdown/prose": "^7.2.0"
  },
  "dependencies": {
    "clsx": "^2.0.0",
    "tslib": "^2.5.0"
  },
  "devDependencies": {
    "tailwind-nord": "^1.2.0",
    "@milkdown/core": "7.5.0",
    "@milkdown/ctx": "7.5.0",
    "@milkdown/prose": "7.5.0"
  },
  "nx": {
    "targets": {
      "build": {
        "outputs": [
          "{projectRoot}/lib"
        ],
        "dependsOn": [
          "build"
        ]
      },
      "tsc": {
        "outputs": [],
        "dependsOn": [
          "build"
        ]
      }
    }
  },
  "scripts": {
    "start": "concurrently -n es,dts \"vite build --watch\"  \"tsc --emitDeclarationOnly --watch\"",
    "tsc": "tsc --noEmit && echo",
    "build": "tsc --emitDeclarationOnly && vite build"
  },
  "types": "./lib/index.d.ts"
}